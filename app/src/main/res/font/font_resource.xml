<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- EB GARAMOND -->
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/eb_garamond_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/eb_garamond_regular" />
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/eb_garamond_bold"
        android:textStyle="bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/eb_garamond_bold" />

    <!-- MISSION GOTHIC -->
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/mission_gothic_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/mission_gothic_regular" />
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/mission_gothic_bold"
        android:textStyle="bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/mission_gothic_bold" />
</font-family>